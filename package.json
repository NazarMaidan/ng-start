{
  "name": "ngx-levi9",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:scss && npm run lint:html",
    "lint:ts": "eslint --color -c .eslintrc --ext .ts .",
    "lint:scss": "npx stylelint \"src/**/*.scss\" --syntax scss",
    "lint:html": "npx htmlhint \"src\" --config .htmlhintrc",
    "e2e": "ng e2e",
    "bundle:report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/ngx-levi9/stats.json",
    "build:prod": "ng build --prod",
    "compodoc": "./node_modules/.bin/compodoc --theme laravel -p src/tsconfig.app.json",
    "format:write": "prettier \"./{src,e2e}/**/*.{ts,js,html,scss}\" --write",
    "format:test": "prettier \"./{src,e2e}/**/*.{ts,js,html,scss}\" --list-different"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.2",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "^11.0.2",
    "@angular/compiler": "^11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/forms": "^11.0.2",
    "@angular/material": "^11.0.0",
    "@angular/platform-browser": "^11.0.2",
    "@angular/platform-browser-dynamic": "^11.0.2",
    "@angular/router": "^11.0.2",
    "bootstrap": "^4.5.3",
    "core-js": "^2.6.11",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular/cli": "^11.0.2",
    "@angular/compiler-cli": "^11.0.2",
    "@angular/language-service": "^11.0.2",
    "@angular-eslint/eslint-plugin": "^0.6.0-beta.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "codelyzer": "^6.0.1",
    "htmlhint": "^0.14.2",
    "husky": "^1.3.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "stylelint": "~13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "~4.2.0",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-scss": "~3.18.0",
    "ts-node": "~7.0.0",
    "typescript": "4.0.5",
    "webpack-bundle-analyzer": "^3.9.0",
    "tslint": "~6.1.3",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1"
  }
}
