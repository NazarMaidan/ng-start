{
  "name": "binck",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle:report": "webpack-bundle-analyzer dist/binck/stats.json",
    "app:info": "npx ng-app-counter",
    "build:prod": "npm run app:info && ng build --prod --aot --build-optimizer --vendor-chunk --stats-json --named-chunks --extract-css=false",
    "compodoc": "./node_modules/.bin/compodoc --theme laravel -p src/tsconfig.app.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "private": true,
  "dependencies": {
